aprilData$`9`$Reading = cumsum(aprilData$`9`$Reading)
monitor1April = ggplot(aprilData$`1`, aes(x = Date, y = Reading)) + geom_line() + ggtitle("Monitor 1") + ylim(0, 25000)
monitor2April = ggplot(aprilData$`2`, aes(x = Date, y = Reading)) + geom_line() + ggtitle("Monitor 2") + ylim(0, 25000)
